[cdn-rhel-8-for-x86_64-baseos-rpms]
name = CDN - Red Hat Enterprise Linux 8 for x86_64 - BaseOS (RPMs)
baseurl = https://cdn.redhat.com/content/dist/rhel8/8/x86_64/baseos/os
enabled = 0
gpgcheck = 1
gpgkey = file:///etc/pki/rpm-gpg/RPM-GPG-KEY-redhat-release
sslverify = 1
sslcacert = /etc/rhsm/ca/redhat-uep.pem
sslclientkey = /etc/pki/entitlement/1234567890123456789-key.pem
sslclientcert = /etc/pki/entitlement/1234567890123456789.pem
metadata_expire = 86400
enabled_metadata = 1

